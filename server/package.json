  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon -r dotenv/config --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' index.ts",
    "copy-protos": "bash-scripts/copy-protos.sh",
    "build": "tsc --build && npm run copy-protos",
    "production": "npm run build && node bin/www",
    "clean": "tsc --build --clean",
    "docker:up": "docker-compose -p pear-connect --env-file .env up -d",
    "docker:down": "docker-compose -p pear-connect  --env-file .env down",
    "db:cli": "mongo admin -u admin -p password",
    "envoy": "envoy -c envoy.config.yaml",
    "lint": "npx eslint . --ext .ts",
    "lint-ci": "eslint --max-warnings=0 . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npx eslint . --ext .ts --fix"
  },
